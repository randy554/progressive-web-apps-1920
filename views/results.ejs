<%- include('./partials/header.ejs') %>

<h2>Je hebt <%= yourScore %> van de 12 goed!</h2>

<main id="category">

    <% Object.entries(userAnswers).forEach((object, index) => { %>
    <div class="questionBlock">
        <p class="quizQuestions">Vraag <%= index+1 %>: <%= apiData[index].question %></p>
<!--        <p><%= object[0] + ': ' + object[1] %></p>-->
        <p>Jouw antwoord: <%= object[1] %></p>
        <p class="True">Juiste antwoord: <%= apiData[index].correct_answer %></p>
    </div>
       <% }); %>

</main>

<%- include('./partials/footer.ejs') %>
